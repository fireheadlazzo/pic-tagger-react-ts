{
  "name": "pic-tagger",
  "version": "1.0.0",
  "description": "image server focusing on ease of categorization by contents",
  "main": "build/index.js",
  "scripts": {
    "prebuild": "trash build",
    "build": "tsc -p tsconfig.json",
    "postbuild": "npm run copy-json",
    "start": "node build/index.js",
    "test": "echo 'no test script'",
    "copy-json": "cpx src/**/*.json build",
    "letsgo": "npm run build && npm run start"
  },
  "author": "Anthony Magno",
  "license": "GPL-3.0-or-later",
  "_moduleAliases": {
    "config": "build/config",
    "controllers": "build/controllers",
    "models": "build/models",
    "services": "build/services"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.7.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "nconf": "^0.11.0",
    "pg": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/multer": "^1.4.5",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.14.14",
    "@types/pg": "^7.14.7",
    "@types/uuid": "^8.3.0",
    "cpx": "^1.5.0",
    "nodemon": "^2.0.6",
    "trash-cli": "^4.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
